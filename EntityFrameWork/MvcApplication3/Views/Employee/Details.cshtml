@model MvcApplication3.Models.Employee

@{
    ViewBag.Title = "Details";
}

<h2>Employee Details </h2>
<table>
    <tr>
        <td>
            <b>Employe id:</b>
        </td>
        <td>
            <b>@Model.EmployeeId</b>
        </td>
    </tr>
    <tr>
        <td>
            <b>Employe name:</b>
        </td>
        <td>
            <b>@Model.Name</b>
        </td>
    </tr>
    <tr>
        <td>
            <b>Employe Garde:</b>
        </td>
        <td>
            <b>@Model.Gender</b>
        </td>
    </tr>
    <tr>
        <td>
            <b>Employe City:</b>
        </td>
        <td>
            <b>@Model.City</b>
        </td>
    </tr>

</table>
